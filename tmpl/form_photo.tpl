	<div class="prop-photos">
		<table border="0" cellpadding="0" cellspacing="0" width="100%">
			<tbody align="center" valign="top">
				<!-- BEGIN GALLERY_ROW -->
				<tr>
					<!-- BEGIN GALLERY_PHOTO -->
					<td colspan="1" width="50%">
						<div>
							<img src="{PHOTO_SRC}" alt="{ALT_TEXT}" title="{ALT_TEXT}">
